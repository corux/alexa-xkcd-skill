{
  "name": "alexa-xkcd-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "build": "npm-run-all --parallel build:skill build:utterances",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "build:utterances": "mkdir -p build && bin/utterances --schema model/schema.json --utterances model/UTTERANCES > build/UTTERANCES",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "package": "bestzip build/package.zip build/index.js build/UTTERANCES",
    "predeploy": "npm run package",
    "deploy": "bin/deploy --config config/lambda.config.js --package build/package.zip"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "^9.0.1",
    "alexa-request": "^0.2.1",
    "alexa-response": "^4.0.1",
    "alexa-ssml-jsx": "^1.0.1",
    "alexa-utterances": "^0.2.1",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bestzip": "^2.1.1",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "eslint": "^5.6.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "node-aws-lambda": "^0.1.8",
    "nomnom": "^1.8.1",
    "npm-run-all": "^4.1.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "webpack": "^3.1.0"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
